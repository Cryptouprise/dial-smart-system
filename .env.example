# Supabase Configuration
# Get these values from your Supabase project settings at https://app.supabase.com
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# External API Keys (Optional - Configure in app settings)
# Retell AI - AI Voice Conversations
VITE_RETELL_API_KEY=

# Go High Level - CRM Integration
VITE_GHL_API_KEY=

# Yellowstone - Lead Data Integration
VITE_YELLOWSTONE_API_KEY=

# Telnyx - Phone Number Provider
VITE_TELNYX_API_KEY=

# Application Settings
VITE_APP_NAME=Dial Smart System
VITE_APP_ENV=development

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SPAM_DETECTION=true
VITE_ENABLE_NUMBER_ROTATION=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
